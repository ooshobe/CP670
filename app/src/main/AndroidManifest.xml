<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworld">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/assignment1_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/assignment1_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloWorld">
        <activity android:name=".WeatherForcast"></activity>
        <activity
            android:name=".TestToolbar"
            android:label="@string/title_activity_test_toolbar"
            android:theme="@style/Theme.HelloWorld.NoActionBar" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ChatWindow" />
        <activity android:name=".ListItemsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>